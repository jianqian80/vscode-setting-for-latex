    "python.linting.pylintArgs": [
        "--generated-members"
    ],
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.synctex.afterBuild.enabled": true,
    "latex-workshop.message.badbox.show": false,
    "editor.wordWrap": "on",
